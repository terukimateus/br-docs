{
  "name": "br-docs",
  "version": "1.1.12",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+:https//github.com/terukimateus/br-docs.git"
  },
  "homepage": "http://github.com/terukimateus/br-docs#readme",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./joi": {
      "types": "./dist/joi/index.d.ts",
      "default": "./dist/joi/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "joi": [
        "dist/joi/index.d.ts"
      ],
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "vitest",
    "lint": "eslint ./src --ext .ts,.tsx",
    "coverage": "vitest run --coverage"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "brasil",
    "utils",
    "cpf",
    "phone",
    "cep",
    "cnpj",
    "pix",
    "boleto",
    "cnh",
    "nis",
    "uuid",
    "email",
    "documentos",
    "validacao",
    "formatacao"
  ],
  "author": "Mateus Teruki",
  "description": "Utilitários para validação e formatação de documentos brasileiros como CPF, telefone, CEP e CNPJ.",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.50.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "joi": "^17.0.0"
  },
  "peerDependencies": {
    "joi": "^17.0.0"
  },
  "peerDependenciesMeta": {
    "joi": {
      "optional": true
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
